	<text>
		<name>Registration text</name>
		<offsets>
			<x-m>-1.646</x-m>
			<y-m>-0.434</y-m>
			<z-m>1.035</z-m>
			<pitch-deg>-5.3</pitch-deg>
		</offsets>
		<alignment>left-bottom</alignment>
		<axis-alignment>yz-plane</axis-alignment>
		<type type="string">text-value</type>
		<format type="string">%s</format>
		<property>sim/model/livery/registration</property>
		<font>helvetica_bold.txf</font>
		<character-size>0.03</character-size>
		<character-aspect-ratio type="double">1.2</character-aspect-ratio>
		<font-resolution>
			<width type="int">32</width>
			<height type="int">32</height>
		</font-resolution>
	</text>
	<animation>
		<type>material</type>
		<object-name>Registration text</object-name>
		<ambient>
			<red>0.7</red>
			<green>0.7</green>
			<blue>0.7</blue>
		</ambient>
	</animation>

	<!-- ANIMATIONS -->
	<!-- Lighting -->
	<animation>
		<name>Lighting</name>
		<type>material</type>
		<condition>
			<greater-than>
				<property>systems/electrical/outputs/cockpit-lights</property>
				<value>15</value>
			</greater-than>
		</condition>
		<emission>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.5</blue>
		</emission>
	</animation>

	<!-- Glareshield -->
	<animation>
		<name>Flight director switch pick</name>
		<type>pick</type>
		<object-name>FDSwitches</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>autopilot/settings/flight-director</property>
			</binding>
		</action>
	</animation>
	<animation>
		<name>Flight director switch rotate</name>
		<type>rotate</type>
		<object-name>FDSwitches</object-name>
		<property>autopilot/settings/flight-director</property>
		<interpolation>
			<entry><ind>0</ind><dep>35</dep></entry>
			<entry><ind>1</ind><dep>-35</dep></entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>-1.403</x-m>
			<y-m>0</y-m>
			<z-m>1.207</z-m>
		</center>
	</animation>

	<!-- Yokes -->
	<animation>
		<name>Elevator yoke movement</name>
		<type>rotate</type>
		<object-name>LeftYoke</object-name>
		<object-name>RightYoke</object-name>
		<object-name>YokeBases</object-name>
		<property>controls/flight/elevator</property>
		<factor>20</factor>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>-1.304</x-m>
			<y-m>0</y-m>
			<z-m>0.072</z-m>
		</center>
	</animation>
	<animation>
		<name>Aileron left yoke movement</name>
		<type>rotate</type>
		<object-name>LeftYoke</object-name>
		<property>controls/flight/aileron</property>
		<factor>90</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>-0.565</y-m>
			<z-m>0.895</z-m>
		</center>
	</animation>
	<animation>
		<name>Aileron right yoke movement</name>
		<type>rotate</type>
		<object-name>RightYoke</object-name>
		<property>controls/flight/aileron</property>
		<factor>90</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>0</x-m>
			<y-m>0.565</y-m>
			<z-m>0.895</z-m>
		</center>
	</animation>

	<!-- Rudder pedals -->
	<animation>
		<name>Left rudder pedals</name>
		<type>translate</type>
		<object-name>LeftPedals</object-name>
		<property>controls/flight/rudder</property>
		<interpolation>
			<entry><ind>-1</ind><dep>-0.07</dep></entry>
			<entry><ind>1</ind><dep>0.28</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<name>Right rudder pedals</name>
		<type>translate</type>
		<object-name>RightPedals</object-name>
		<property>controls/flight/rudder</property>
		<interpolation>
			<entry><ind>1</ind><dep>-0.07</dep></entry>
			<entry><ind>-1</ind><dep>0.28</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- Armrests -->
	<animation>
		<name>Pilot armrest pick</name>
		<type>pick</type>
		<object-name>LeftArmrests</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				 ATR_72.doors.toggle("pilot-armrests");
				</script>
			</binding>
		</action>
	</animation>
	<animation>
		<name>Pilot armrest rotate</name>
		<type>rotate</type>
		<object-name>LeftArmrests</object-name>
		<property>sim/model/door-positions/pilot-armrests/position-norm</property>
		<factor>95</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
		<center>
			<x-m>-0.4</x-m>
			<y-m>0</y-m>
			<z-m>0.75</z-m>
		</center>
	</animation>
	<animation>
		<name>Copilot armrest pick</name>
		<type>pick</type>
		<object-name>RightArmrests</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
